{
  "name": "@oslo-flanders/html-respec-generator",
  "version": "0.0.6-alpha.0",
  "description": "Generates an HTML respec file using an OSLO-compliant RDF file",
  "author": "Digitaal Vlaanderen <https://data.vlaanderen.be/id/organisatie/OVO002949>",
  "homepage": "https://github.com/informatievlaanderen/OSLO-UML-Transformer/tree/main/packages/oslo-generator-respec-html#readme",
  "license": "MIT",
  "main": "index.js",
  "_moduleAliases": {
    "@oslo-generator-respec-html": "./lib",
    "@oslo-core": "node_modules/@oslo-flanders/core/lib",
    "@oslo-converter-stakeholders": "node_modules/@oslo-flanders/stakeholders-converter/lib"
  },
  "bin": {
    "oslo-generator-respec": "./bin/runner.js"
  },
  "directories": {
    "lib": "lib",
    "bin": "bin"
  },
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "lib/**/*.njk",
    "bin/**/*.d.ts",
    "bin/**/*.js",
    "index.d.ts",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Informatievlaanderen/OSLO-UML-Transformer.git"
  },
  "scripts": {
    "build": "npm run build:ts",
    "build:ts": "tsc"
  },
  "bugs": {
    "url": "https://github.com/Informatievlaanderen/OSLO-UML-Transformer/issues"
  },
  "dependencies": {
    "@oslo-flanders/core": "^0.0.6-alpha.0",
    "@oslo-flanders/html-respec-generator": "^0.0.6-alpha.0",
    "@oslo-flanders/stakeholders-converter": "^0.0.8-alpha.0",
    "inversify": "^6.0.1",
    "module-alias": "^2.2.3",
    "n3": "^1.16.4",
    "nunjucks": "^3.2.4",
    "reflect-metadata": "^0.1.13",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@rdfjs/types": "^1.1.0",
    "@types/nunjucks": "^3.2.2"
  }
}
