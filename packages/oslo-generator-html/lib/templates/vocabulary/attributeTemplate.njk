<h2> {{attribute.label}} </h2>

<table>
  <tr>
    <th>URI</th>
    <td>{{attribute.id | urlize() | safe}}</td>
  </tr>

  <tr>
    <th>Definition</th>
    {% if attribute.definition%}
    <td>{{attribute.definition}}</td>
     {% else %}
    <td style="color:red;">Empty</td>
    {%endif%}
  </tr>

  <tr>
    <th>Label</th>
    {% if attribute.label%}
    <td>{{attribute.label}}</td>
     {% else %}
    <td style="color:red;">Empty</td>
    {%endif%}
  </tr>


  {% if attribute.usageNote %}
  <tr>
    <th>UsageNote</th>
    <td>{{attribute.usageNote}}</td>
  </tr>
  {% endif %}

  <tr>
    <th>Domain</th>
    {% if attribute.domain %}
    <td>{{attribute.domain | urlize() | safe}}</td>
    {% else %}
    <td style="color:red;">Empty</td>
    {% endif %}
  </tr>

  <tr>
    <th>Range</th>
    {% if attribute.range %}
    <td>{{attribute.range | urlize() | safe}}</td>
    {% else %}
    <td style="color:red;">Empty</td>
    {% endif %}
  </tr>

</table>