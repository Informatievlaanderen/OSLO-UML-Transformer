<h3>
  {{property.label}}
</h3>

<table class='simple'>
  <tr>
    <td>Type</td>
    <td>Eigenschap</td>
  </tr>
  <tr>
    <td>URI</td>
    <td>{{property.id}}</td>
  </tr>
  {% if property.parent %}
    <tr>
      <td>Parent</td>
      <td>{{property.parent}}</td>
    </tr>
  {% endif %}
  <tr>
    <td>Definition</td>
    <td>{{property.definition}}</td>
  </tr>
  {% if property.usageNote %}
    <tr>
      <td>Usage</td>
      <td>{{property.usageNote}}</td>
    </tr>
  {% endif %}
  <tr>
    <td>Domain</td>
    <td>{{property.domain}}</td>
  </tr>
  <tr>
    <td>Range</td>
    <td>{{property.range}}</td>
  </tr>
</table>