{
  "name": "@oslo-flanders/shacl-template-generator",
  "version": "0.0.0",
  "description": "> TODO: description",
  "author": "Dwight Van Lancker <dwight.vanlancker@vlaanderen.be>",
  "homepage": "https://github.com/informatievlaanderen/OSLO-UML-Transformer/tree/main/packages/oslo-generator-shacl-template#readme",
  "license": "MIT",
  "main": "index.js",
  "_moduleAliases": {
    "@oslo-generator-shacl-template": "./lib",
    "@oslo-core": "node_modules/@oslo-flanders/core/lib"
  },
  "bin": {
    "oslo-shacl-template-generator": "./bin/runner.js"
  },
  "directories": {
    "lib": "lib",
    "bin": "bin",
    "test": "test"
  },
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "bin/**/*.d.ts",
    "bin/**/*.js",
    "index.d.ts",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Informatievlaanderen/OSLO-UML-Transformer.git"
  },
  "jest": {
    "runner": "groups",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "/test/.*.test.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "index.js",
      "test"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@oslo-core/(.*)$": "<rootDir>/../oslo-core/lib/$1",
      "^@oslo-generator-shacl-template/(.*)$": "<rootDir>/lib/$1"
    }
  },
  "scripts": {
    "build": "npm run build:ts",
    "build:ts": "tsc",
    "test:integration": "node \"../../node_modules/jest/bin/jest.js\" --group=integration",
    "test:unit": "node \"../../node_modules/jest/bin/jest.js\" --group=unit"
  },
  "bugs": {
    "url": "https://github.com/Informatievlaanderen/OSLO-UML-Transformer/issues"
  },
  "dependencies": {
    "@oslo-flanders/core": "^0.0.6-alpha.0",
    "i18n": "^0.15.1",
    "inversify": "^6.0.1",
    "module-alias": "^2.2.3",
    "n3": "^1.17.2",
    "rdf-data-factory": "^1.1.2",
    "rdf-serialize": "^2.2.3",
    "reflect-metadata": "^0.2.1",
    "streamify-array": "^1.0.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/i18n": "^0.13.10",
    "@types/n3": "^1.16.4",
    "jest-runner-groups": "^2.2.0",
    "rdf-parse": "^2.3.3",
    "streamify-string": "^1.0.1"
  }
}
